                      "type": "object",
                      "properties": {
                        "-salis": {
                          "type": "string", "pattern": "^[A-Z]*$"
                        },
                        "adresas": {
                          "type": "object",
                          "properties": {
                            "miestas": {
                              "type": "string",
                              "enum": ["Niujorkas", "amber", "green"]
                            },
                            "gatve": {
                              "type": "string", "minLength": 2, "maxLength": 3
                            },
                            "namas": {

                              "anyOf": [
                                {
                                  "type": "string",
                                  "maxLength": 5
                                },
                                {
                                  "type": "number",
                                  "minimum": 0
                                }
                              ]
                            }
                          },
                          "required": [
                            "miestas",
                            "gatve",
                            "namas"
                          ]
                        }
                      },
                      "required": [
                        "-salis",
                        "adresas"
                      ]
                    },
                    "darbuotojuSkaicius": {
                      "type": "integer"
                    },
                    "akcijosKaina": {
                      "type": "object",
                      "properties": {
                        "-valiuta": {
                          "type": "string"
                        },
                        "-salis": {
                          "type": "string"
                        },
                        "#text": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "-valiuta",
                        "-salis",
                        "#text"
                      ]
                    },
                    "susikurimoData": {
                      "type": "string", "format": "date"
                    }
                  },
                  "required": [
                    "-pavadinimas",
                    "vadovas",
                    "centrinisStabas",
                    "darbuotojuSkaicius",
                    "akcijosKaina",
                    "susikurimoData"
                  ]
